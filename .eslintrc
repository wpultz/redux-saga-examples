{
  "parser": "babel-eslint",
  "rules": {
    "no-restricted-syntax": [0],
    "react/jsx-indent": [2, 2],
    "react/jsx-indent-props": [2, 2],
    "react/jsx-a11y/href-no-hash": [0],
    "indent": [2, 2, { SwitchCase: 1 }],
    "comma-dangle": [2, "never"],
    "new-cap": [0],
    "no-unused-vars": [2, {
      "vars": "all",
      "argsIgnorePattern": "^_",
      "varsIgnorePattern": "^_"
    }]
  }
}
